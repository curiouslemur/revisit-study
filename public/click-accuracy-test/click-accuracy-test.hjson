{
    $schema: https://raw.githubusercontent.com/reVISit-studies/study/main/src/parser/schema.json
    studyMetadata: {
        title: Click Accuracy Test
        version: pilot
        authors: [
            The reVISit Team
        ]
        date: 2023-06-15
        description: A demo of using React dynamic stimuli, and receiving input from it.
        organizations: [
            University of Utah
            WPI
            University of Toronto
        ]
    }
    recordStudyAudio: true
    uiConfig: {
        contactEmail: test@test.com
        helpTextPath: click-accuracy-test/help.md
        logoPath: assets/revisitLogoSquare.svg
        withProgressBar: true
        autoDownloadStudy: false
        sidebar: true
    }
    components: {
        introduction: {
            type: markdown
            path: click-accuracy-test/introduction.md
            response: []
        }
       
        trial: {
            meta: {
                nr-dots: 1
            }
            title: Click Accuracy Test
            description: try to click on the center of the moving dot
            instruction: Click on the moving dot
            type: react-component
            path: ClickAccuracyTest.tsx
            parameters: {
                speed: 100
                taskid: accuracy
            }
            nextButtonLocation: sidebar
            response: [
                {
                    id: accuracy
                    prompt: Your click distance to circle center
                    required: true
                    location: sidebar
                    type: iframe
                }
            ]
        }

    }
    sequence: { 
        order: fixed
        components:  [
            introduction
            trial
        ]
    }
}
