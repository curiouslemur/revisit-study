    "@mantine/form": "^6.0.7",
    "@mantine/hooks": "^5.8.0",
    "@quentinroy/latin-square": "^1.1.1",
    "@reduxjs/toolkit": "^1.9.3",
    "@swc/helpers": "^0.4.14",
    "@tabler/icons-react": "^2.15.0",
    "@trrack/core": "^1.1.0",
    "@trrack/redux": "^0.0.6",
    "@trrack/vis-react": "^1.2.3",
    "@types/hjson": "^2.4.3",
    "@types/howler": "^2.2.10",
    "@types/lodash": "^4.14.199",
