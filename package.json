    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.5",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.10.0",
    "ts-json-schema-generator": "^1.2.0",
    "typedoc": "^0.24.8",
    "typedoc-plugin-frontmatter": "^0.0.2",
    "typedoc-plugin-markdown": "^3.15.4",
